{
  "name": "paladin-protocol",
  "version": "1.0.0",
  "description": "Paladin Smart Contracts",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "npx hardhat clean",
    "compile": "npx hardhat compile",
    "test": "npx hardhat test",
    "coverage": "npx hardhat coverage --network hardhat --solcoverjs .solcover.js",
    "deploy_kovan": "npx hardhat run --network kovan scripts/deploy/all_deploy.ts",
    "deploy": "npx hardhat run --network mainnet scripts/deploy/all_deploy.ts"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@nomiclabs/buidler": "^1.4.8",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^2.1.3",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@typechain/ethers-v5": "^7.0.0",
    "@typechain/hardhat": "^2.0.0",
    "@types/chai": "^4.2.18",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.14.1",
    "chai": "^4.3.4",
    "ethereum-waffle": "^3.3.0",
    "ethers": "^5.1.4",
    "hardhat": "^2.3.2",
    "hardhat-contract-sizer": "^2.0.3",
    "hardhat-gas-reporter": "^1.0.4",
    "solidity-coverage": "^0.7.16",
    "ts-generator": "^0.1.1",
    "ts-node": "^9.1.1",
    "typechain": "^5.0.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "dotenv": "^10.0.0"
  },
  "resolutions": {
    "@ethereumjs/block": "3.2.1",
