{
  "name": "paladin-protocol",
  "version": "1.0.0",
  "description": "Paladin Smart Contracts",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "npx hardhat clean",
    "compile": "npx hardhat compile",
    "test": "npx hardhat test",
    "coverage": "npx hardhat coverage --network hardhat --solcoverjs .solcover.js",
    "deploy_kovan": "npx hardhat run --network kovan scripts/deploy/all_deploy.ts",
    "deploy": "npx hardhat run --network mainnet scripts/deploy/all_deploy.ts"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@nomiclabs/buidler": "^1.4.8",
    "@nomiclabs/hardhat-ethers": "^2.0.6",
    "@nomiclabs/hardhat-etherscan": "^3.1.0",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "^6.1.2",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.0.0",
    "chai": "^4.3.6",
    "ethereum-waffle": "^3.4.4",
    "ethers": "^5.6.8",
    "hardhat": "^2.9.9",
    "hardhat-contract-sizer": "^2.5.1",
    "hardhat-gas-reporter": "^1.0.8",
    "solidity-coverage": "^0.7.21",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.8.1",
    "typechain": "^8.1.0",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "dotenv": "^16.0.1"
  },
  "resolutions": {
    "@ethereumjs/block": "3.2.1",
